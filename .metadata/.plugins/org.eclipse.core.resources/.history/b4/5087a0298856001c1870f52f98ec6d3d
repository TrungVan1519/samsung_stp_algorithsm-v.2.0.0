package stp.arrays.binhthuong_testiq;

import java.io.FileInputStream;
import java.util.Scanner;

import stp.utils.MyUtils;

public class Solution {

	static char[][] arr;
	
	public static void main(String[] args) {
		try {
			System.setIn(new FileInputStream(MyUtils.getAbsolutePath("arrays\\binhthuong_testiq")));
			Scanner s = new Scanner(System.in);
			
			arr = new char[4][4];
			
			// input
			for (int i = 0; i < 4; i++) {
				String str = s.next();
				for (int j = 0; j < 4; j++) {
					arr[i][j] = str.charAt(j);
				}
			}
			MyUtils.showMatrix(arr, 4, 4);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
