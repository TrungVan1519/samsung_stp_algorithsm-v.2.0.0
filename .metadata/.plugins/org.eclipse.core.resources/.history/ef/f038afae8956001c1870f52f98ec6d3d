package stp.arrays.binhthuong_testiq;

import java.io.FileInputStream;
import java.util.Scanner;

import stp.utils.MyUtils;

public class Solution {

	static int size = 4;
	static char[][] arr;

	static int count;

	public static void main(String[] args) {
		try {
			System.setIn(new FileInputStream(MyUtils.getAbsolutePath("arrays\\binhthuong_testiq")));
			Scanner s = new Scanner(System.in);

			arr = new char[size][size];
			count = 0;

			// cách giải:
			// nếu tồn tại hình vuông 2x2 chứa 2 ký tự '#' và 2 ký tự '.' thì in 'NO'
			// còn lại thì in 'YES'

			// input
			for (int i = 0; i < size; i++) {
				String str = s.next();
				for (int j = 0; j < size; j++) {
					arr[i][j] = str.charAt(j);
				}
			}

			// solve
			for (int i = 0; i < size - 1; i++) {
				for (int j = 0; j < size - 1; j++) {
					if (arr[i][j] + arr[i + 1][j + 1] + arr[i + 1][j] + arr[i][j + 1] != 2 * ('#' + '.')) {
						count++;
					}
				}
			}

			if (count > 0) {
				System.out.println("YES");
			} else {
				System.out.println("NO");
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
