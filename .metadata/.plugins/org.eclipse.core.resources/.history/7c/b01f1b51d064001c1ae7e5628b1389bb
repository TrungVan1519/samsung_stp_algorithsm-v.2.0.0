package again2._001hotelfloors;

import java.io.FileInputStream;
import java.util.Scanner;

import utils.MyUtils;

public class Main {

	static int T;

	static int row, col;
	static char[][] arr;

	static float cntPeople, cntRooms;

	public static void main(String[] args) throws Exception {
		System.setIn(
				new FileInputStream(MyUtils.getAbsolutePath("/Project/src/again2/_001hotelfloors/input.txt", "mac")));
		Scanner s = new Scanner(System.in);

		T = s.nextInt();
		for (int tc = 0; tc < T; tc++) {
			row = s.nextInt();
			col = s.nextInt();
			arr = new char[row][col];

			// input
			cntPeople = 0;
			for (int i = 0; i < row; i++) {
				String str = s.next();
				for (int j = 0; j < col; j++) {
					arr[i][j] = str.charAt(j);
					if (arr[i][j] == '*') {
						cntPeople++;
					}
				}
			}

			// solve
			cntRooms = 0;
			for (int i = 1; i < row; i++) {
				for (int j = 1; j < col; j++) {
					if (arr[i][j] != '#') {
						cntRooms++;
						fillRoom(i, j);
					}
				}
			}

//			System.out.println(String.format("%.2f", cntPeople / cntRooms));
			System.out.printf("%.2f\n", cntPeople / cntRooms);
		}
	}

	private static void fillRoom(int i, int j) {
		if (arr[i + 1][j] != '#') {
			arr[i + 1][j] = '#';
			fillRoom(i + 1, j);
		}

		if (arr[i - 1][j] != '#') {
			arr[i - 1][j] = '#';
			fillRoom(i - 1, j);
		}

		if (arr[i][j + 1] != '#') {
			arr[i][j + 1] = '#';
			fillRoom(i, j + 1);
		}

		if (arr[i][j - 1] != '#') {
			arr[i][j - 1] = '#';
			fillRoom(i, j - 1);
		}
	}
}
