package stp.strings.thisamsunglan1;

import java.io.FileInputStream;
import java.util.Scanner;

import utils.MyUtils;

public class Solution {

	static int T;

	static char[] p, s;

	public static void main(String[] args) {
		try {
			System.setIn(new FileInputStream(MyUtils.getAbsolutePath("stp\\strings\\thisamsunglan1")));
			Scanner sc = new Scanner(System.in);

			T = sc.nextInt();
			for (int tc = 1; tc <= T; tc++) {
				// input
				String strP = sc.next();
				p = new char[strP.length()];
				for (int i = 0; i < p.length; i++) {
					p[i] = strP.charAt(i);
				}

				String strS = sc.next();
				s = new char[strS.length()];
				for (int i = 0; i < s.length; i++) {
					s[i] = strS.charAt(i);
				}

				// solve
				for (int i = 0; i < p.length; i++) {
					if (p[i] == '.') {
						if (p[i + 1] == '*') {
							splitArr(p, i);
						} else {

						}
					}
				}
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

	private static void splitArr(char[] p2, int i) {
		char[] first = new char[i];
		char[] second = new char[p2.length - 1 - i + 2];

		
	}
}
